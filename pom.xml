        </license>
    </licenses>

    <developers>
        <developer>
            <id>davey.wu</id>
            <name>darui.wu</name>
            <email>darui.wu@163.com</email>
        </developer>
    </developers>

    <properties>
        <spring.version>5.2.0.RELEASE</spring.version>
        <release.plugin.version>2.4.1</release.plugin.version>
        <test4j-mock.version>1.0.1</test4j-mock.version>
        <test4j-utils.version>1.0.1</test4j-utils.version>
        <test4j-assert.version>1.0.0</test4j-assert.version>
