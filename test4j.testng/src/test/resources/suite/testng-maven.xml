<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="test4j">
	<test name="test4j">
		<groups>
			<run>
				<include name="all-test" />
				<exclude name="broken" />
				<exclude name="hibernate" />
				<exclude name="broken-install" />
				<exclude name="memery-db" />
				<exclude name="for-test" />
				<exclude name="testng-demo" />
				<exclude name="manual" />
				<exclude name="debug" />
			</run>
		</groups>
		<packages>
			<package name="org.test4j.*" />
		</packages>
	</test>
	<listeners>
		<listener class-name="org.test4j.testng.report.UserTestReporter"></listener>
	</listeners>
</suite>